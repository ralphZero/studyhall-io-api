    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "firebase-admin": "^10.2.0",
    "firebase-functions": "^4.4.0",
    "joi": "^17.9.2",
    "moment": "^2.29.4",
